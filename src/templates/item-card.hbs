{{#each this}}
<li class="link-gaps">
  <article class="articles">
    <img src="{{urlToImage}}" alt="articles-news-img" class="card-img" />
    <div class="card-content">
      <h3 class="card-header">{{title}}</h3>
      <p class="card-text">
       {{description}}
      </p>
      <div class="card-time">
        <p>{{publishedAt}}</p>
        <a href="#">Read more</a>
      </div>
    </div>
  </article>
</li> 
{{/each}}
